{
  "name": "hismacomp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://a-a-m-k.github.io/HiSMaComp",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build && echo 'Bundle analysis generated at dist/bundle-analysis.html'",
    "preview": "vite preview",
    "prepare": "husky",
    "lint": "npx lint-staged",
    "prettier": "npx prettier --write .",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "perf:audit": "lighthouse http://localhost:5173 --output=html --output-path=./lighthouse-report.html",
    "perf:budget": "npm run build && npm run perf:audit"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.2",
    "@mui/material": "^6.4.2",
    "html2canvas": "^1.4.1",
    "maplibre-gl": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "^8.0.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/babel__traverse": "^7.20.7",
    "@types/node": "^22.15.30",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "gh-pages": "^6.3.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^16.1.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "^4.0.0",
    "vite": "^6.0.5",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
